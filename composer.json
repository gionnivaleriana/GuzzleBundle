{
    "name": "gionnivaleriana/guzzle-bundle",
    "description": "Kopjra Guzzle 5 Bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Emanuele Minotto",
            "email": "minottoemanuele@gmail.com",
            "homepage": "http://www.emanueleminotto.it"
        },
        {
            "name": "Joy Lazari",
            "email": "joy.lazari@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~5.2",
        "php": ">= 5.4"
    },
    "require-dev": {
        "guzzlehttp/cache-subscriber": "~0.1",
        "guzzlehttp/guzzle-services": "~0.5",
        "guzzlehttp/log-subscriber": "~1.0",
        "guzzlehttp/oauth-subscriber": "~0.2",
        "guzzlehttp/retry-subscriber": "~2.0",
        "emanueleminotto/guzzle-cache-subscriber": "~1.0",
        "phpunit/phpunit": "~4.5",
        "symfony/monolog-bundle": "~2.4",
        "symfony/symfony": "~2.5"
    },
    "suggest": {
        "emanueleminotto/guzzle-cache-subscriber": "provide the server side caching subscriber",
        "guzzlehttp/cache-subscriber": "provide the client side caching subscriber",
        "guzzlehttp/guzzle-services": "allows guzzle services definition",
        "guzzlehttp/log-subscriber": "provide the log subscriber",
        "guzzlehttp/oauth-subscriber": "provide the OAuth 1 subscriber",
        "guzzlehttp/retry-subscriber": "provide the retry system subscriber"
    },
    "autoload": {
        "psr-4": {
            "Kopjra\\GuzzleBundle\\": ""
        }
    }
}
