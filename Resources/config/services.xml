<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="kpj_guzzle.class">GuzzleHttp\Client</parameter>
        <parameter key="kpj_guzzle.event.emitter.class">GuzzleHttp\Event\Emitter</parameter>
        <parameter key="kpj_guzzle.service.class">GuzzleHttp\Command\Guzzle\GuzzleClient</parameter>
    </parameters>
    <services>
        <service class="%kpj_guzzle.class%" id="kpj_guzzle">
            <argument/>
        </service>
        <service class="%kpj_guzzle.event.emitter.class%" id="kpj_guzzle.event.emitter"/>
    </services>
</container>
