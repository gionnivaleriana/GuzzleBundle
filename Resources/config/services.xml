<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="guzzle" class="GuzzleHttp\Client">
      <argument></argument>
    </service>
    <service id="kopjra.guzzle_bundle.twig.guzzle_extension" class="Kopjra\GuzzleBundle\Twig\GuzzleExtension" public="false">
      <tag name="twig.extension"/>
      <argument type="service" id="guzzle"/>
    </service>
    <service id="kopjra.guzzle_bundle.event_listener.emitter" class="Kopjra\GuzzleBundle\EventListener\Emitter" public="false"/>
  </services>
</container>
