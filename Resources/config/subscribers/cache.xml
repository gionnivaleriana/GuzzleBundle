<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="kopjra_guzzle.subscribers.cache.class">Kopjra\GuzzleBundle\Subscribers\CacheSubscriber</parameter>
        <parameter key="kopjra_guzzle.subscribers.cache.provider">kopjra_guzzle.cache_subscriber.default_provider</argument>
        <parameter key="kopjra_guzzle.subscribers.cache.type">client</argument>
    </parameters>
    <services>
        <service class="%kopjra_guzzle.subscribers.cache.class%" id="kopjra_guzzle.subscribers.cache" public="false">
            <argument id="%kopjra_guzzle.subscribers.cache.provider%" type="service" />
            <argument>%kopjra_guzzle.subscribers.cache.type%</argument>
            <tag name="guzzle.subscriber"/>
        </service>
        <service class="Doctrine\Common\Cache\ArrayCache" id="kopjra_guzzle.subscribers.cache.default_provider">
    </services>
</container>