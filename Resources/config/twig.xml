<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="kpj_guzzle.twig.guzzle_extension.class">Kopjra\GuzzleBundle\Twig\GuzzleExtension</parameter>
    </parameters>
    <services>
        <service class="%kpj_guzzle.twig.guzzle_extension.class%" id="kpj_guzzle.twig.guzzle_extension" public="false">
            <argument id="kpj_guzzle" type="service"/>
            <tag name="twig.extension"/>
        </service>
    </services>
</container>
